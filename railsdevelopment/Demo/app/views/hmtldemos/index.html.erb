<h1>Listing hmtldemos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hmtldemos.each do |hmtldemo| %>
      <tr>
        <td><%= hmtldemo.title %></td>
        <td><%= hmtldemo.body %></td>
        <td><%= link_to 'Show', hmtldemo %></td>
        <td><%= link_to 'Edit', edit_hmtldemo_path(hmtldemo) %></td>
        <td><%= link_to 'Destroy', hmtldemo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hmtldemo', new_hmtldemo_path %>
